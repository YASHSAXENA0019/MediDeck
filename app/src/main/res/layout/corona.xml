<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/getin_logo"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#222831"
    android:backgroundTint="#222831"
    tools:context=".patient_home_page">


    <Button
        android:id="@+id/covid_hos"
        android:layout_width="300dp"
        android:layout_height="100dp"
        android:layout_alignParentTop="false"
        android:drawableRight="@drawable/covid19_hospitals"
        android:text="@string/covid_19_ca"
        android:textAppearance="@style/covid_19_ca"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="565dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="300dp"
        android:layout_height="100dp"
        android:layout_alignParentTop="false"
        android:drawableRight="@drawable/rtcpr_logo"
        android:text="@string/laboratorie"
        android:textAppearance="@style/covid_19_ca"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="440dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="300dp"
        android:layout_height="100dp"
        android:layout_alignParentTop="false"
        android:drawableRight="@drawable/rtpcr_icon"
        android:text="Vaccine"
        android:textAppearance="@style/covid_19_ca"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="315dp" />
</androidx.constraintlayout.widget.ConstraintLayout>