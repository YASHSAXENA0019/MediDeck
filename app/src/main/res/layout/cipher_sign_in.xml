<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#222831">

    <TextView
        android:id="@+id/get_on_boar"
        android:layout_width="240dp"
        android:layout_height="46dp"
        android:layout_marginTop="60dp"
        android:gravity="center"
        android:text="@string/get_on_boar"
        android:textAppearance="@style/get_on_boar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.895"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/name_si"
        android:layout_width="132dp"
        android:layout_height="65dp"
        android:gravity="center"
        android:text="@string/name"
        android:textAppearance="@style/name_si"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="180dp" />

    <EditText
        android:id="@+id/name_si_edittext"
        android:layout_width="240dp"
        android:layout_height="65dp"
        android:background="@drawable/input_profile_draw"
        android:elevation="16dp"
        tools:layout_editor_absoluteX="155dp"
        tools:layout_editor_absoluteY="180dp" />

    <TextView
        android:id="@+id/number_si"
        android:layout_width="132dp"
        android:layout_height="65dp"
        android:gravity="center"
        android:text="@string/number"
        android:textAppearance="@style/name_si"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="260dp" />

    <EditText
        android:id="@+id/number_si_edittext"
        android:layout_width="240dp"
        android:layout_height="65dp"
        android:background="@drawable/input_profile_draw"
        android:elevation="16dp"
        tools:layout_editor_absoluteX="155dp"
        tools:layout_editor_absoluteY="260dp" />

    <TextView
        android:id="@+id/age_si"
        android:layout_width="132dp"
        android:layout_height="65dp"
        android:gravity="center"
        android:text="@string/age"
        android:textAppearance="@style/name_si"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="340dp" />

    <EditText
        android:id="@+id/age_si_edittext"
        android:layout_width="240dp"
        android:layout_height="65dp"
        android:background="@drawable/input_profile_draw"
        android:elevation="16dp"
        tools:layout_editor_absoluteX="155dp"
        tools:layout_editor_absoluteY="340dp" />


    <TextView
        android:id="@+id/email_si"
        android:layout_width="132dp"
        android:layout_height="65dp"
        android:gravity="center"
        android:text="@string/email"
        android:textAppearance="@style/name_si"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="420dp" />

    <EditText
        android:id="@+id/email_si_edittext"
        android:layout_width="240dp"
        android:layout_height="65dp"
        android:background="@drawable/input_profile_draw"
        android:elevation="16dp"
        tools:layout_editor_absoluteX="155dp"
        tools:layout_editor_absoluteY="420dp" />

    <TextView
        android:id="@+id/password_si"
        android:layout_width="132dp"
        android:layout_height="65dp"
        android:gravity="center"
        android:text="@string/password"
        android:textAppearance="@style/name_si"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="500dp" />

    <EditText
        android:id="@+id/password_si_edittext"
        android:layout_width="240dp"
        android:layout_height="65dp"
        android:background="@drawable/input_profile_draw"
        android:elevation="16dp"
        tools:layout_editor_absoluteX="155dp"
        tools:layout_editor_absoluteY="500dp" />

    <TextView
        android:id="@+id/c_password_si"
        android:layout_width="132dp"
        android:layout_height="65dp"
        android:gravity="center"
        android:text="@string/c_password"
        android:textAppearance="@style/name_si"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="580dp" />

    <EditText
        android:id="@+id/c_password_si_edittext"
        android:layout_width="240dp"
        android:layout_height="65dp"
        android:background="@drawable/input_profile_draw"
        android:elevation="16dp"
        tools:layout_editor_absoluteX="155dp"
        tools:layout_editor_absoluteY="580dp" />

</androidx.constraintlayout.widget.ConstraintLayout>